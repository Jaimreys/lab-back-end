<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lpc.dao.RoleMenuMapper">
    <select id="selectMenuByRole" resultType="java.lang.Integer" parameterType="int">
        select menu_id
        from role_menu
        where role_id = #{role};
    </select>
</mapper>